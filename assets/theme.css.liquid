.product-media__gallery--grid {
  display: flex;
  flex-wrap: wrap;
}
.product-image__grid {
  padding: 0 10px 10px 0;
}
.product-image__grid:nth-child(4n + 1),
.product-image__grid:nth-child(4n + 5){
  flex: 0 0 100%;
}
.product-image__grid:nth-child(4n + 2),
.product-image__grid:nth-child(4n + 3),
.product-image__grid:nth-child(4n + 4){
  flex: 0 0 33.33%;
}
.product-image__grid iframe {
    aspect-ratio: 1/1;
}
.rating {
  display: flex;
  align-items: center;
}
.rating .rating__caption {
  font-size: 12px;
  text-transform: capitalize;
}
.product-full__info--btn {
  padding-top: 20px;
}
.product-full__info--btn a {
  font-size: 14px;
  border-bottom: 1px solid #111111;
  display: inline-block;
}
.product__sticky-wrapper {
  position: fixed;
  left: 0;
  z-index: 99;
  width: 100%;
  background: white;
  padding: 15px 0;
}
.sticky__cart-position--top{
  top: 69px;
  border-bottom: 1px solid #E1E3E5;
}
.sticky__cart-position--bottom{
  bottom: 0;
  border-top: 1px solid #E1E3E5;
}
.product__sticky-form {
  display: flex;
  align-items: center !important;
  justify-content: space-between;
}
.product__sticky-form--content {
  display: flex;
  align-items: center;
  gap: 20px;
}
.product__sticky-form--form {
  display: flex;
  align-items: center;
  gap: 20px;
}
.sticky-sale__price{
  font-size: 14px;
}
.sticky-product__title h3{
  text-transform: capitalize;
}
.product__sticky-image {
  width: 60px;
  height: 60px;
}
.product__sticky-form--selectBox .product-select__options {
  display: flex;
  gap: 10px;
}
.sticky-product-form__select {
  display: none;
}
.sticky-product-form__select select {
  padding: 12px;
}
@media only screen and (min-width: 769px) {
  .product-grid {
    display: flex;
    gap: 60px;
    align-items: flex-start;
  }
  .product__image-container {
    padding-bottom: 0;
    position: sticky;
    top: 20px;
  }
  .product-grid__reverse{
    flex-direction: row-reverse;
  }
  .product-block__small {
    width: 40%;
  }
  .product-block__half{
    width: 50%;
  }
  .product-block__large {
    width: 60%;
  }
  .product-block__extended{
    width: 65%;
  }
