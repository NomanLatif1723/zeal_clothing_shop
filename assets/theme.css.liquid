  gap: 25px;
}
.product-media__grid .product-media__wrapper {
  width: 85%;
  height: auto;
}
.product-media__grid .product-media__thumbnails {
  width: 15%;
  margin: 0;
  max-height: 600px;
}
.product-media__grid .swiper-button-next,
.product-media__grid .swiper-button-prev {
  left: 50% !important;
  transform: translateX(-50%) !important;
  right: auto !important;
  height: 30px !important;
}
.product-media__grid .swiper-button-next {
  top: 100% !important;
  margin-top: -40px !important;
}
.product-media__grid .swiper-button-prev {
  top: 0 !important;
}
.product-media__grid .swiper-vertical .swiper-button-prev:after,
.product-media__grid .swiper-vertical .swiper-button-next:after{
  transform: rotate(90deg);
}
button[name="add"]:disabled {
