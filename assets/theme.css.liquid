  }
  .btn-text__dark{
    color: var(--colorBodyText);
    border-color: var(--colorBodyText);
  }
  .btn-text__light{
    color: #ffffff;
    border-color: #ffffff;
  }
  
  .btn__outline {
      border: 1px solid;
  }
  
  .btn__block {
      background: #FCEAEA;
      color: #9d1339;
      border: 1px solid #fceaea;
  }
  
  .banner__text {
      font-size: 14px;
      line-height: 30px;
      opacity: 0.65;
      padding-top: 0;
  }
  .banner-text__container {
    padding-bottom: 25px;
  }
  .promotional-banner__content {
    text-align: center;
  }
  .banner-button__wrapper .btn__link {
    display: inline-block;
    text-transform: capitalize;
  }
  
  .banner-button__wrapper {
      display: flex;
      justify-content: center;
      gap: 10px;
  }
  .banner__heading h2 {
      font-size: 18px;
      line-height: 30px;
