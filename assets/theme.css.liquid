}
.product__play-icon svg {
  fill: white;
}
.product-media__video {
  width: 100%;
  height: 100%;
}
iframe.product-media__video {
  width: 100%;
  height: 100%;
}
.product__thumbs {
  height: 100%;
}
.product__image-container .swiper-slide {
  height: auto;
