/* resets  */
*,:before ,:after ,input {
  box-sizing: border-box;
}
body,html {
  padding: 0;
  margin: 0;
}
html {
  scroll-behavior: smooth;
}
li {
  list-style: none;
}
a {
  text-decoration: none;
  color: #111111;
  color: var(--colorBodyText);
  display: block;
  transition: all .3s ease;
}
ul {
  margin: 0;
  padding: 0;
}
h1,h2,h3,h4,h5,h6,p,blockquote {
  margin: 0;
}
h1 {
  font-size: 36px;
  line-height: 58px;
}
h2 {
  font-size: 26px;
  line-height: 32px;
  text-transform: uppercase;
}
h3 {
  font-size: 18px;
  line-height: 30px;
  font-weight: 400;
}
h4 {
  font-size: 14px;
  line-height: 18px;
}
img {
  display: block;
  width: 100%;
  height: 100%;
  max-width: 100%;
  object-fit: cover;
  transition: all 0.5s ease;
}
#MainContent {
    min-height: 700px;
}
input,select,textarea {
  background-color: inherit;
  color: inherit;
  /* padding: 12px 15px; */
  padding: 15px;
  border: 1px solid black;
  width: 100%;
  outline: none;
}
button {
  background: none;
  border: none;
  padding: 0;
  cursor: pointer;
}
.rte {
  font-size: 14px;
  line-height: 26px;
  color: rgba(1, 1, 1, 0.6);
}
.rte p {
  margin-bottom: 12px;
}
.rte p:last-child {
  margin-bottom: 0;
}
.icon__close{
  stroke: #111111;
}
.placeholder__image {
  width: 100%;
  height: 100%;
  background: #e1e1e1;
  fill: #999;
  padding: 30px 0;
  max-width: 100%;
  max-height: 100%;
}
.visually__hidden {
  position: absolute;
  width: 1px;
  height: 1px;
  clip: rect(0 0 0 0);
  overflow: hidden;
  margin: -1px;
  padding: 0;
  border: 0;
}
.skip__to-content:focus {
  clip: auto;
  width: auto;
  height: auto;
  margin: 0;
  background: #111111;
  color: #ffffff;
  padding: 10px;
  opacity: 1;
  z-index: 10000;
  transition: none;
}

/* Animations */
.scroll__animate {
  opacity: 0;
  transition: opacity 0.7s ease;
}
.scroll__animate.animate {
  opacity: 1;
}

.customer__forms{
  margin: 30px 0;
}
.success__message {
  background: green;
  color: white;
  padding: 15px;
  margin-bottom: 15px;
}
.errors {
  padding: 15px;
  background: rgba(1,1,1,0.1);
  margin-bottom: 15px;
  color: #dd9191;
}
/* drawers */
.drawer__overlay-container {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100vh;
  z-index: 9;
  background: #000;
  opacity: 0.3;
  visibility: hidden;
  cursor: pointer;
}
.drawer-open__left {
  left: 0 !important;
}
.drawer-open__right {
  right: 0 !important;
}
.drawer__opening{
  overflow-y: hidden;
}
.drawer__header {
  display: flex;
  justify-content: space-between;
  padding: 30px 25px;
  border-bottom: 1px solid rgba(1,1,1,0.1);
}
.drawer-icon__close {
  display: inline-flex;
  cursor: pointer;
}
@media only screen and (min-width: 769px) {
  h1 {
    font-size: 64px;
    line-height: 104px;
  }
  .customer__forms{
    margin: 60px 0;
  }
}
:root {
  --colorBody: {{ settings.body_bg_color }};
  --colorBodyText: {{ settings.body_text_color }};

  --colorButtonPrimary: {{ settings.color_button_primary }};
  --colorButtonTextPrimary: {{ settings.color_button_text_primary }};
  --colorOutlineButton: {{ settings.color_outline_button }};

  --colorButtonSecondary: {{ settings.color_button_secondary }};
  --colorButtonTextSecondary: {{ settings.color_button_text_secondary }};

  --saleBadgeColor: {{ settings.sale_badge_color }};
  --soldoutBadgeColor: {{ settings.soldout_badge_color }};

  --bodyTextFont: {{ settings.body_font_family.family }};
  --headingTextFont: {{ settings.heading_font_family.family }};

  --bodyBackgroundColor: {{ settings.body_bg_color }};
  --bodyTextColor: {{ settings.body_text_color }};

  /* not used yet */
  --borderColor: {{ settings.border_color }};
}
body {
  font-family: var(--bodyTextFont), sans-serif;
  background: var(--colorBody);
  color: var(--colorBodyText);
}
h1,h2,h3,h4,h5,h6 {
  font-family: var(--headingTextFont), sans-serif;
}
button {
  font-family: var(--bodyTextFont), sans-serif;
}
.page__width {
  max-width: 1480px;
  margin: 0 auto;
  padding: 0 17px;
}
.btn {
  padding: 14px 22px;
  margin-bottom: 10px;
  text-align: center;
  cursor: pointer;
  transition: all 0.3s ease;
}
/* .btn:last-child {
    margin: 0;
} */
.btn__primary {
  background: #111111;
  background: var(--colorButtonPrimary);
  color: #ffffff;
  color: var(--colorButtonTextPrimary);
  border: 1px solid #111111;
  border: 1px solid var(--colorButtonPrimary);
}
.btn__link {
  font-size: 13px;
  line-height: 16px;
  letter-spacing: 0.7px;
  text-transform: uppercase;
}
.btn__secondary {
  background: #9d1339;
  background: var(--colorButtonSecondary);
  color: #ffffff;
  color: var(--colorButtonTextSecondary);
  border: 1px solid #9d1339;
  border: 1px solid var(--colorButtonSecondary);
}
.btn__primary:hover, .btn__secondary:hover {
  background: #ffffff;
  color: #111111;
  border: 1px solid #111111;
}
.btn__outline:hover {
  background: #111111;
  color: #ffffff;
}
.main__heading {
  padding-bottom: 6px;
}
@media only screen and (min-width: 769px) {
  .page__width {
    padding: 0 40px;
  }
  .btn {
    margin-bottom: 0;
  }
  .btn__link {
    display: inline-block;
  }
}

/* Back To Top Button */
.back__to-top {
  position: fixed;
  z-index: 9;
  bottom: 120px;
  
}
.back__arrow-right{
  right: 30px;
}
.back__arrow-left{
  left: 30px;
}
.back__arrow-center{
  left: 50%;
  transform: translateX(-50%);
}
.back__to-top a {
  /* background: var(--colorButtonSecondary); */
  padding: 12px 15px;
}
.back__to-top svg {
  fill: #ffffff;
}
.back__to-top:hover svg {
  fill: #111111;
}
/* loader */
@-webkit-keyframes spin {
  0% { -webkit-transform: rotate(0deg); }
  100% { -webkit-transform: rotate(360deg); }
}

@keyframes spin {
  0% { transform: rotate(0deg); }
  100% { transform: rotate(360deg); }
}
.loader {
  position: fixed;
  top: 0;
  background: #0003;
  width: 100%;
  height: 100vh;
  left: 0;
  z-index: 9999;
}
.loading__spinner {
  width: 30px;
  height: 30px;
  -webkit-animation: spin 2s linear infinite; /* Safari */
  animation: spin 1s linear infinite;
  position: relative;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
}
.loading__spinner circle {
  opacity: 0.25;
}

/* text positions css */
.text__position-top-left {
  vertical-align: top;
  text-align: left;
}
.text__position-top-center {
  vertical-align: top;
  text-align: center;
}
.text__position-top-right {
  vertical-align: top;
  text-align: right;
}
.text__position-center-left {
  vertical-align: middle;
  text-align: left;
}
.text__position-center {
  vertical-align: middle;
  text-align: center;
}
.text__position-center-right {
  vertical-align: middle;
  text-align: right;
}
.text__position-bottom-left {
  vertical-align: bottom;
  text-align: left;
}
.text__position-bottom-center {
  vertical-align: bottom;
  text-align: center;
}
.text__position-bottom-right {
  vertical-align: bottom;
  text-align: right;
}

/* text alignment css */
.text__align-center {
  text-align: center;
}
.text__align-left {
  text-align: left;
}
.text__align-right {
  text-align: right;
}

/* Languages and currencies */
.disclosure__wrapper {
  display: flex;
  align-items: center;
  gap: 15px;
}
.disclosure__button {
  color: var(--colorAnnouncementText);;
  display: flex;
  align-items: center;
  gap: 10px;
  cursor: pointer;
}
.disclosure__button svg {
  fill: var(--colorAnnouncementText);
}
.disclosure__item a {
  color: var(--colorAnnouncementText);;
  font-size: 13px;
}
.disclosure {
  position: relative;
}

.disclosure__list {
  position: absolute;
  background: var(--colorAnnouncement);
  z-index: 9;
  width: 200px;
  padding: 15px;
  right: 0;
  top: 32px;
}
.disclosure__item {
  padding-bottom: 15px;
  display: flex;
  align-items: center;
  gap: 10px;
}
.disclosure__item:last-child {
  padding-bottom: 0;
}
.disclosure__item-flags {
  width: 20px;
}

/* Announcement Bar  */
.announcement__bar {
  background: var(--colorAnnouncement);
  color: var(--colorAnnouncementText);
  padding: 10px 0;
}
.announcement__wrapper {
  text-align: center;
}
.main__timer {
  font-size: 14px;
  color: #EDC128;
  font-weight: bold;
}
.main__text {
  display: none;
}
.social__container {
  display: none;
}
.social__icons-list li {
  display: inline-block;
}
.social__icons-list a {
  color: var(--colorAnnouncementText);
  display: block;
  padding: 5px;
}
.social__icons-list .icon {
  width: 18px;
  height: 18px;
  fill: var(--colorAnnouncementText);
  position: relative;
  transition: transform 0.3s ease;
}
.social__icons-list li:hover >a .icon {
  transform: translateY(-6px);
}
.announcement__heading {
  font-size: 13px;
  padding-bottom: 6px;
}
.social__fallback-text {
  display: none;
}

.language__currency-container {
  display: none;
}
@media only screen and (min-width: 769px) {
