/* @charset "UTF-8"; */

/* resets  */
*,:before ,:after ,
input {
  box-sizing: border-box;
}

body,
html {
  padding: 0;
  margin: 0;
}

li {
  list-style: none;
}

a {
  text-decoration: none;
  color: #111111;
  color: var(--colorBodyText);
  display: block;
}
ul {
  margin: 0;
  padding: 0;
}

h1,
h2,
h3,
h4,
h5,
h6,
p,
blockquote {
  margin: 0;
}
h1 {
  font-size: 36px;
  line-height: 58px;
}
h2 {
  font-size: 26px;
  line-height: 42px;
  text-transform: uppercase;
}

h3 {
  font-size: 18px;
  line-height: 30px;
  font-weight: 400;
}

h4 {
  font-size: 14px;
  line-height: 18px;
}

img {
  display: block;
  width: 100%;
  height: 100%;
  max-width: 100%;
}

input,
select,
textarea {
  background-color: inherit;
  color: inherit;
  padding: 12px 15px;
  border: 1px solid black;
}
.rte {
  font-size: 14px;
  line-height: 26px;
  color: rgba(1, 1, 1, 0.6);
}

.rte p {
  margin-bottom: 12px;
}

.placeholder__image {
  width: 100%;
  height: 100%;
  background: #e1e1e1;
  fill: #999;
  padding: 30px 0;
  max-width: 100%;
  max-height: 100%;
}

.visually__hidden {
  position: absolute;
  width: 1px;
  clip: rect(0 0 0 0);
  overflow: hidden;
  margin: -1px;
}


@media only screen and (min-width: 769px) {
  h1 {
    font-size: 64px;
    line-height: 104px;
  }
  .rte p {
    margin-bottom: 0;
    display: inline;
  }
