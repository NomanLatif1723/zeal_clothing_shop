  .right-text__block .image-text__content:nth-child(2) {
    padding-left: 40px;
  }
  .image-text__content {
    padding-bottom: 70px;
  }
}

/* Image With Text Section */
.image__block {
    padding-bottom: 15px;
}

.image-text__button--container .btn {
    text-align: center;
}

.image-text__heading h2 {
    text-transform: capitalize;
}
.image-text__text {
    padding-bottom: 25px;
}
@media only screen and (min-width: 769px) {
  .image-text__wrapper {
    display: flex;
    align-items: center;
    gap: 60px;
  }
  .image-text__reverse {
    flex-direction: row-reverse;
  }
  .image__block {
    padding-bottom: 0;
  }
  .image-text__text {
    max-width: 500px;
    display: inline-block;
  }
  .image-text__item {
      width: 50%;
  }
}
