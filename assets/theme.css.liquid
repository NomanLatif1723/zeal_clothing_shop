  }
}
@media only screen and (min-width: 769px) and (max-width: 992px){
  .banner-text__container {
    max-width: 400px;
  }
}
/* Promotional Block Section */

.promo-block__item {
    display: flex;
    min-height: 280px;
    padding-bottom: 10px;
}

.promo-block__image {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
}
.promo-sub__heading {
    text-transform: uppercase;
}
.promo-main__heading {
    padding-bottom: 25px;
}
.promo-main__heading h2 {
    text-transform: capitalize;
    font-size: 20px;
    line-height: 32px;
}
.btn__promo {
    display: inline-block;
    text-transform: capitalize;
}
.promo-block__image .promo__image {
    object-fit: cover;
}
.promo-block__content {
    position: relative;
    display: table;
    height: 100%;
    width: 100%;
}
.promo-block__container {
    position: relative;
    background-repeat: no-repeat;
    overflow: hidden;
    width: 100%;
}

.promo-block__box {
    display: table-cell;
    padding: 15px 25px;
}

.promo-block__shadow {
    display: inline-block;
    max-width: 320px;
}

@media only screen and (max-width: 768px){
  .promo__image-hide{
    display: none;
  }
}
@media only screen and (min-width: 769px) and (max-width: 992px) {
  .promo-block__item {
    flex: 1 0 50%;
  }
}
@media only screen and (min-width: 993px) {
  .promo-block__item {
    flex: 1 0 33.33%;
  }
}
@media only screen and (min-width:769px) {
  .promo-block__grid {
    display: flex;
    flex-wrap: wrap;
  }
  .promo-main__heading h2 {
    font-size: 26px;
    line-height: 42px;
  }
  .promo-main__heading {
    padding-bottom: 30px;
  }
  .promo-image__mobile{
    display: none;
  }
  .promo-block__grid {
    margin-top: -20px;
    margin-left: -20px;
  }
  .promo-block__item {
    padding: 20px 0 0 20px;
    min-height: 300px;
  }
  .promo-block__shadow {
    max-width: 400px;
  }
}

/*  Lookbook Section */
.lookbook-grid__item {
  padding-bottom: 10px;
}
.lookbook-image__container {
    height: 100%;
    width: 100%;
}
.lookbook-image__container img {
    object-fit: cover;
}
@media only screen and (min-width: 769px) {
  .lookbook-grid {
    display: flex;
    flex-wrap: wrap;
  }
  .lookbook-grid__item {
      flex: 1 0 33.33%;
  }
  .lookbook-grid__item:nth-child(4n+4) {
      flex: 1 0 100%;
  }
}

/* Featured Collection */
.featured-collection__grid {
    display: flex;
    flex-wrap: wrap;
    margin-left: -10px;
}

.featured-grid__item {
    padding-left: 10px;
    padding-bottom: 20px;
    width: 100%;
}

.collection__header {
    padding-bottom: 35px;
}
.featured-grid__image img {
    object-fit: cover;
}
.featured-grid__image {
    margin-bottom: 15px;
    position: relative;
}

.grid-product__title h4 {
    text-transform: uppercase;
    letter-spacing: 0.7px;
    font-weight: 400;
}

.grid-product__title {
    padding-bottom: 10px;
}
.grid-product__price {
    font-size: 13px;
    color: var(--colorBodyText);
}
.product__sale-price{
  padding-right: 6px;
}
.product__compare-price {
    text-decoration: line-through;
    opacity: 0.6;
}
.grid-product__vendor {
    text-transform: uppercase;
    font-size: 13px;
    line-height: 16px;
    color: var(--colorBodyText);
    opacity: 0.6;
    padding-bottom: 10px;
    letter-spacing: 0.7px;
}
.product-secondary__image {
    display: none;
}
.has-secondary__image:hover .product__image {
    display: none;
}

.has-secondary__image:hover .product-secondary__image {
    display: block;
}

.featured-grid__content {
    position: relative;
}

.grid__badge {
    position: absolute;
    right: 0;
    top: 0;
    z-index: 9;
}

.badge {
    padding: 4px 8px;
    display: block;
    text-transform: uppercase;
    font-size: 12px;
    line-height: 14px;
    font-weight: 700;
    letter-spacing: 0.7px;
   
}
.sale__badge{
  background: var(--saleBadgeColor);
}

.soldout__badge{
  background: var(--soldoutBadgeColor);
}
.badge-color__dark{
   color: black;
}

.badge-color__light{
   color: white;
}

.quick-shop__wrapper {
    position: absolute;
    bottom: 0;
    width: 100%;
    padding: 6px;
    visibility: hidden;
    transition: all 0.3s ease;
}

.quick-shop__flex {
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding: 14px 28px;
    background: rgba(255,255,255,0.5);
    cursor: pointer;
}

.quick-shop__text {
    font-size: 14px;
    text-transform: uppercase;
    line-height: 16px;
    font-weight: 700;
    letter-spacing: 0.7px;
}

.featured-grid__image:hover .quick-shop__wrapper {
    visibility: visible;
}

.color-swatch__item label {
    width: 20px;
    height: 20px;
    display: block;
    padding: 2px;
    border: 1px solid rgba(1,1,1,0.3);
    transition: all 0.3s ease;
    cursor: pointer;
}

input[type="radio"]:checked + label {
    border: 1px solid rgba(1,1,1,1);
}

.color-swatch__option {
    display: block;
    width: 100%;
    height: 100%;
}
.color-swatch__list {
    display: flex;
    align-items: center;
    justify-content: center;
    gap: 6px;
}

.show-more__swatches {
    width: 20px;
    height: 20px;
    font-size: 18px;
    cursor: pointer;
}
.show-more__swatches.hide{
  display: none;
}
.color-swatch__item input {
    display: none;
}

.grid-product__variants {
    padding-top: 15px;
}
.hidden__swatches {
    display: none;
}
.hidden__swatches.show {
    display: inline-flex;
}
@media only screen and (max-width: 1200px){
  .block__item--auto{
    width: 25%;
  }
}
@media only screen and (max-width: 768px) {
  .block__item--auto{
    width: 50%;
  }
  .list-scroller {
    flex-wrap: nowrap;
    overflow-x: auto;
  }
  
  .list-scroller__item {
      flex: 1 0 50%;
  }
}

@media only screen and (min-width: 1201px){
  .block__item--half{
    width: 50%;
  }
  .block__item--one-third{
      width: 33.33%;
  }
  .block__item--one-fourth{
      width: 25%;
  }
  .block__item--one-fifth{
      width: 20%;
  }
}
@media only screen and (min-width: 769px) {
  .featured-collection__grid {
    margin-left: -20px;
  }
  .featured-grid__item {
    padding-left: 20px;
  }
