{%-  liquid 
    assign class = classes | escape
    assign image_element = img-element
    assign alt = img-element.alt | escape 

-%}
<img loading="lazy"
  src="{{ image_element | img_url: img-width  }}"
  width="{{ img-width }}"
  height="{{ img-height }}"
  class="{{ class }}"
  alt="{{ alt }}">
{% comment %} <img
loading="lazy"
width=img-width
height=""
class=class
src="{{ image_element | img_url: img-width }}"
srcset=""
alt=alt> {% endcomment %}