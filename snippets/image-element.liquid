{%-  liquid 
    assign class = classes | escape
    assign image_element = img-element  
    assign alt = image_element.alt | default: ''
-%}

<img loading="lazy"
  src="{{ image_element | img_url: img-width  }}"
  width="{{ img-width }}"
  height="{{ img-height }}"
  class="{{ class }}"
  alt="{{ alt }}"
  srcset="{{ img_widths }}"
  >